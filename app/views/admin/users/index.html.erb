<div class="container">
  <h1>会員一覧</h1>
    <table class='table table-hover table-inverse'>
      <thead>
        <tr>
          <th>会員ID</th>
          <th>会員名</th>
          <th>利用中</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
            <td><%= link_to @users.name,admin_user_path(user.id) %></td>
            <td></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  <%= paginate @users %>
</div>